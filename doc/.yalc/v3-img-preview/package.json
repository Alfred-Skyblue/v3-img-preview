{
  "name": "v3-img-preview",
  "version": "1.1.1",
  "description": "vue3图片预览插件",
  "private": false,
  "author": {
    "name": "ypt",
    "email": "yangpanteng@gmail.com"
  },
  "keywords": [
    "vue3",
    "img",
    "images",
    "图片",
    "图片预览"
  ],
  "license": "MIT",
  "homepage": "https://github.com/Alfred-Skyblue/v3-img-preview",
  "main": "dist/v3-img-preview.ssr.js",
  "browser": "dist/v3-img-preview.esm.js",
  "module": "dist/v3-img-preview.esm.js",
  "unpkg": "dist/v3-img-preview.min.js",
  "types": "dist/types/src/entry.esm.d.ts",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "prebuild": "rimraf ./dist",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "postbuild": "rimraf ./dist/types/dev ./dist/types/src/entry.d.ts"
  },
  "dependencies": {
    "@vueuse/core": "^8.1.1"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alfred-Skyblue/v3-img-preview"
  },
  "peerDependencies": {
    "vue": "^3.2.25"
  },
  "engines": {
    "node": ">=12"
  },
  "yalcSig": "c270e86734606b29ea580bd0c08b5f52"
}
